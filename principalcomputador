package interface1;

public class PrincipalComputador {

	public static void main(String[] args) {

		Gamer gamer = new Gamer();
		Home home = new Home();
		
		System.out.println("Gamer:");
		gamer.ligar();
		gamer.reiniciar();
		gamer.desligar();
		gamer.carregandoSistema();
		
		System.out.println("Home:");
		home.ligar();
		home.reiniciar();
		home.desligar();
		home.carregandoSistema();
			
	}
}
